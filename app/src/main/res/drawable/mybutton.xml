<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#7C795E2C" />
            <gradient android:angle="-90" android:startColor="#7C795E2C" android:endColor="#795E2C"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#A1795E2C" />
            <solid android:color="#A6795E2C"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#A1795E2C" />
            <gradient android:angle="-90" android:startColor="#9CA37E3A" android:endColor="#90795E2C" />
        </shape>
    </item>
</selector>